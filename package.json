{
  "name": "wsdl-to-ts",
  "version": "0.2.4",
  "description": "Build TypeScript typings for SOAP WSDL",
  "main": "lib/wsdl-to-ts.js",
  "module": "esm/wsdl-to-ts.js",
  "types": "esm/wsdl-to-ts.d.ts",
  "scripts": {
    "test": "node test/index.js",
    "build": "tsc -p tsconfig.json && npx tsc -p tsconfig.json -d --outDir esm --module es2015 --target es2016",
    "ci": "npm run build && npm test"
  },
  "bin": {
    "wsdl-to-ts": "lib/index.js"
  },
  "keywords": [
    "soap",
    "typescript",
    "wsdl",
    "ts"
  ],
  "author": {
    "name": "TimLuq",
    "url": "https://github.com/TimLuq"
  },
  "license": "ISC",
  "peerDependencies": {
    "type-fest": "^4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/lodash": "^4.14.121",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": ">=0.5.1 <1.0.0",
    "@types/node": "^11.15.54",
    "@types/request": "^2.48.5",
    "@types/sax": "^1.2.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.0.7",
    "flatted": "^3.0.5",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mkdirp": ">=0.5.1 <1.0.0",
    "reflect-metadata": "^0.1.13",
    "soap": "1.6.0"
  }
}
